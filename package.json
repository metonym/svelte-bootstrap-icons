{
  "name": "svelte-bootstrap-icons",
  "version": "3.2.0",
  "license": "MIT",
  "description": "Bootstrap SVG icons as Svelte components",
  "author": "Eric Liu (https://github.com/metonym)",
  "svelte": "./lib/index.js",
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "exports": {
    ".": {
      "types": "./lib/index.d.ts",
      "svelte": "./lib/index.js"
    },
    "./lib/*.svelte": {
      "types": "./lib/*.svelte.d.ts",
      "import": "./lib/*.svelte"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "build:lib": "svelvg glob=bootstrap-icons/icons iconIndex",
    "test": "bun test"
  },
  "devDependencies": {
    "@sveltejs/vite-plugin-svelte": "^5.0.3",
    "bootstrap-icons": "1.12.1",
    "culls": "^0.1.1",
    "svelte": "^5.28.2",
    "svelvg": "latest",
    "vite": "^6.3.4"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/metonym/svelte-bootstrap-icons.git"
  },
  "homepage": "https://metonym.github.io/svelte-bootstrap-icons/",
  "bugs": "https://github.com/metonym/svelte-bootstrap-icons/issues",
  "keywords": [
    "svelte",
    "svelte component",
    "bootstrap",
    "icon",
    "svg"
  ],
  "files": [
    "lib"
  ],
  "type": "module"
}
