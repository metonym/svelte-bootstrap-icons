{
  "name": "svelte-bootstrap-icons",
  "version": "1.4.0",
  "license": "MIT",
  "description": "Bootstrap SVG icons as Svelte components",
  "author": "Eric Liu (https://github.com/metonym)",
  "svelte": "./lib/index.js",
  "main": "./lib/index.js",
  "types": "./types/index.d.ts",
  "sideEffects": false,
  "scripts": {
    "dev": "rollup -cw",
    "predeploy": "rollup -c",
    "deploy": "npx gh-pages -d dist",
    "prepack": "s2s --input=node_modules/bootstrap-icons/icons && npx sveld --glob",
    "test": "svelte-check --workspace=test"
  },
  "devDependencies": {
    "bootstrap-icons": "1.4.1",
    "rollup": "^2.44.0",
    "svelte-check": "^1.4.0",
    "svelte-readme": "^3.1.0",
    "svg-to-svelte": "^2.2.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/metonym/svelte-bootstrap-icons.git"
  },
  "homepage": "https://github.com/metonym/svelte-bootstrap-icons",
  "bugs": "https://github.com/metonym/svelte-bootstrap-icons/issues",
  "keywords": [
    "svelte",
    "svelte component",
    "bootstrap",
    "icon",
    "svg"
  ],
  "files": [
    "lib",
    "types"
  ]
}
