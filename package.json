{
  "name": "svelte-bootstrap-icons",
  "version": "1.7.2",
  "license": "MIT",
  "description": "Bootstrap SVG icons as Svelte components",
  "author": "Eric Liu (https://github.com/metonym)",
  "svelte": "./lib/index.js",
  "main": "./lib/index.js",
  "types": "./types/index.d.ts",
  "type": "module",
  "sideEffects": false,
  "scripts": {
    "dev": "rollup -cw",
    "predeploy": "rollup -c",
    "deploy": "gh-pages -d dist",
    "prepack": "s2s --input=node_modules/bootstrap-icons/icons && sveld --glob",
    "test": "svelte-check --workspace test"
  },
  "devDependencies": {
    "bootstrap-icons": "1.8.0",
    "gh-pages": "^3.2.3",
    "rollup": "^2.66.0",
    "sveld": "^0.8",
    "svelte-check": "^2.3.0",
    "svelte-readme": "^3.6.2",
    "svg-to-svelte": "^2.2.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/metonym/svelte-bootstrap-icons.git"
  },
  "homepage": "https://github.com/metonym/svelte-bootstrap-icons",
  "bugs": "https://github.com/metonym/svelte-bootstrap-icons/issues",
  "keywords": [
    "svelte",
    "svelte component",
    "bootstrap",
    "icon",
    "svg"
  ],
  "files": [
    "lib",
    "types"
  ]
}
